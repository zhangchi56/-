/* pages/coach_wall/coach_wall.wxss */
.container{
  padding-bottom: 134rpx;
}
.carousel swiper {
  width: 100%;
  box-sizing: border-box;
  position: relative;
}
.bs{
  font-size: 26rpx;
  color: white;
}

.carousel swiper .slide-image {
  width: 100%;
  height: 100%;
  display: block;
}

/* .wx-swiper-dots {
  position: absolute;
  top: 80%;
} */
.header {
  position: fixed;
  width: 100%;
  top: 0;
  left: 0;
  height: 44px;
  background: #232323;
  z-index: 999;
  text-align: center;
}
.select{
  display: flex;
  margin:44rpx 0 20rpx;
  font-size:30rpx;
color:rgba(255,255,255,0.5);
}
.select_li{
  margin:0 22rpx;
}
.select_active{
  color:#71D3F8;
}
.info_one{
  padding:30rpx 40rpx 30rpx;
  background: #3B3B3B;
  box-sizing: border-box;
}
.info_one .head{
  display: flex;
  align-items: center;
  color:#EFCC59;
  font-size: 30rpx;
  margin-bottom: 15rpx;
}
.info_one .head image{
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  margin-right: 8rpx;
}
.info_one .row{
  margin-top: 15rpx;
    font-size: 24rpx;
  color:rgba(255,255,255,0.5);
}
.info_one .row .title{
  display: flex;
  align-items: center;
  margin-bottom: 6rpx;
  color:#fff;
}
.info_one .row .title .icon{
  width:14rpx;
height:14rpx;
background:rgba(255,255,255,1);
border:2rpx solid rgba(112,112,112,1);
border-radius:50%;
margin-right: 4rpx;
}
.info_one .row .more{
  width:15rpx;
height:15rpx;
transform: rotate(45deg);
border-radius: 2rpx;
border-right: 5rpx solid rgba(255,255,255,0.5);
border-bottom: 5rpx solid rgba(255,255,255,0.5);
margin: 15rpx auto 0;
}
.info_one .row .ellipsis-2{
  display: -webkit-box;
    overflow: hidden;
    text-overflow: ellipsis;
    word-wrap: break-word;
    word-break: break-all;
    white-space: normal !important;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.ellipsis-2{
  display: -webkit-box;
    overflow: hidden;
    text-overflow: ellipsis;
    word-wrap: break-word;
    word-break: break-all;
    white-space: normal !important;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.info_li{
  padding-left: 20rpx;
}
.info_li .title,.free_title{
  font-size:30rpx;
color:rgba(255,255,255,1);
padding-left: 20rpx;
position: relative;
margin:30rpx 0;
}
.info_li .title::before,.free_title::before{
  position: absolute;
  content:'';
width:10rpx;
height:32rpx;
background:rgba(255,255,255,1);
border-radius:60rpx;
left: 0;
top:50%;
margin-top: -16rpx;
}
.info_li .wrap{
  overflow: hidden;
  height: 180rpx;
}
.info_li .wrap_inner{
  display: flex;
  flex-wrap: nowrap;
 overflow:auto;
  padding-bottom: 20rpx;
}
.info_li .wrap_inner image{
  width: 236rpx;
  height: 178rpx;
  margin-right: 40rpx;
  flex-shrink: 0;
}
.info_li .wrap_inner image:last-child{
  padding-right: 20rpx;
}
.info_four .wrap_inner .wrap_inner_li image{
  width: 432rpx;
  height: 288rpx;
  margin-right: 0;
}
.info_four .wrap_inner .wrap_inner_li:last-child image{
  padding-right:40rpx;
}
.info_four .wrap_inner_li .txt{
  box-sizing: border-box;
  width: 432rpx;
  background: #fff;
  padding: 0 20rpx;
}
.info_four .wrap{
height: 464rpx;
}
.info_four .wrap_inner_li{
   margin-right: 40rpx;
}
.info_four .wrap_inner_li .txt_title{
  color:#232323;
  font-size: 30rpx;
  height: 66rpx;
  line-height: 66rpx;
  border-bottom:1px solid rgba(112,112,112,1);
}
.info_four .wrap_inner_li .txt_ct{
  color:#888888;
  font-size: 30rpx;
  padding: 5rpx 0;
}
.info_three .wrap{
    height: 250rpx;
}
.info_three .wrap_inner image{
  width: 236rpx;
  height: 250rpx;
  margin-right: 40rpx;
  flex-shrink: 0;
}
.info_five .content{
  padding: 40rpx 30rpx 0;
  background: #3B3B3B;
}
.info_five .hid{
  max-height:340rpx;
  overflow: hidden;
}
.info_five .list_li{
  margin-bottom: 25rpx;
}
.info_five .list_li_row1{
 color:#fff;
 font-size: 24rpx;
 margin-bottom: 4rpx;
}
.info_five .list_li_row2{
  display: flex;
 color:rgba(255,255,255,0.5);
 font-size: 20rpx;
}
.info_five .list_li_row2 .time{
  margin-left:40rpx;
}
.info_five .more{
  text-align: center;
  padding: 26rpx 0;
  border-top: 2rpx solid rgba(112,112,112,1);
  color:#fff;
  font-size: 20rpx;
}
.bottom{
  display: flex;
  padding: 25rpx 0;
  color:#fff;
  width: 100%;
  position: fixed;
  bottom: 0;
  left:0;
  background: #3B3B3B;
}
.bottom_left,.bottom_right{
  height: 48rpx;
  line-height: 48rpx;
  text-align: center;
  font-size: 30rpx;
  flex:1;
}
.bottom_left{
  color:#EFCC59;
  border-right:2rpx solid rgba(112,112,112,1);
}
.section {
  background: #3b3b3b;
  padding: 40rpx 16rpx;
}

.section_row1 {
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #fff;
  font-size: 30rpx;
  margin-bottom: 28rpx;
}

.section_row1 .nav {
  display: flex;
}

.section_row1 .nav_tab {
  padding: 6rpx 20rpx;
  margin: 0 10rpx;
}

.section_row1 .active {
  background: #07adec;
  border-radius: 10rpx;
}

.section .calendar {
  display: flex;
}

.section .calendar_li {
  flex: 1;
  text-align: center;
  color: rgba(255, 255, 255, 0.5);
  font-size: 24rpx;
}

.section .calendar_li .week {
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  background: rgba(132, 173, 208, 0.5);
  border: 2rpx solid rgba(112, 112, 112, 1);
  border-radius: 50%;
  text-align: center;
  margin: 16rpx auto 0;
  color: rgba(255, 255, 255, 1);
  font-size: 28rpx;
}

.section .calendar_active .week {
  background: #9d9d9d;
  border: 2rpx solid rgba(112, 112, 112, 1);
}

.section .calendar_active .date {
  color: #fff;
}
.gym_head {
  padding: 40rpx 24px 30rpx 16rpx;
  display: flex;
  justify-content: space-between;
}

.gym_head .shop {
  font-size: 30rpx;
  color: #efcc59;
}

.gym_head .place {
  font-size: 26rpx;
  color: #dcdcdc;
}

.gym .li {
  background: rgba(59, 59, 59, 1);
  border: 2rpx solid rgba(112, 112, 112, 0.5);
  padding: 16rpx 40rpx 24rpx 16rpx;
  margin-bottom: 10rpx;
}
.gym .tyk .li_row1{
  margin-bottom: 0;
}
.gym .tyk .li_row1 .left{
align-items: center;
}
.gym .tyk .li_row1 .left .name{
color:#DC324A;
font-size: 32rpx;
}
.gym .li_row1{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 40rpx;
}
.gym .li_row1 .left {
  display: flex;
}
.gym .li_row1 .left image {
  display: block;
  width: 96rpx;
  height: 96rpx;
  background: rgba(0, 0, 0, 0);
  border: 2rpx solid rgba(112, 112, 112, 1);
  border-radius: 50%;
  margin-right: 30rpx;
}

.gym .li_row1 .left .name {
  font-size: 30rpx;
  color: #fff;
}

.gym .li_row1 .left .t2 {
  font-size: 24rpx;
  color: #efcc59;
  margin: 4rpx 0;
}

.gym .li_row1 .left .cost {
  font-size: 24rpx;
  color: #fff;
}
.gym .li_row1 .left .cost text{
  font-size: 28rpx;
  color: #DC324A;
}
.gym .li_row2{
  display: flex;
  justify-content: flex-end;
  align-items: center;
  margin: 8rpx 0;
  color:#fff;
  font-size: 28rpx;
}
.gym .li_row2 .price{
  margin-right: 60rpx;
}
.gym .btn {
  text-align: center;
  width: 158rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 18rpx;
  font-size: 30rpx;
  color: #fff;
}

.gym .btn1 {
  background: #07adec;
}

.gym .btn2 {
  background: #dc324a;
}
.free{
  padding-left:20rpx;
}
